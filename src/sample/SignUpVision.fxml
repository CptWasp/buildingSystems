<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SignUpController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" style="-fx-background-color: #444;" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="nameSigner" layoutX="47.0" layoutY="66.0" prefHeight="25.0" prefWidth="204.0" promptText="Имя Фамилия Отчество" style="-fx-background-color: #333;" />
            <TextField fx:id="emailSigner" layoutX="47.0" layoutY="102.0" prefHeight="25.0" prefWidth="204.0" promptText="Examle@example.com" style="-fx-background-color: #333;" />
            <PasswordField fx:id="passwordSigner" layoutX="47.0" layoutY="137.0" prefHeight="25.0" prefWidth="204.0" promptText="**********" style="-fx-background-color: #333;" />
            <Button fx:id="signUpEndButton" layoutX="47.0" layoutY="175.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="204.0" style="-fx-background-color: #333;" text="Зарегистрироваться" textFill="WHITE" />
         </children>
    </AnchorPane>
  </children>
</VBox>
